{
  "trailingComma": "all",
  "tabWidth": 2,
  "singleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "semi": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^react(.*)$",
    "^react-dom$",
    "<THIRD_PARTY_MODULES>",
    "^@/contexts/(.*)$",
    "^@/components/(.*)$",
    "^@/constants/(.*)$",
    "^@/data/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderCaseInsensitive": true
}