{
  "trailingComma": "all",
  "tabWidth": 2,
  "singleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": true,
  "semi": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "^[react]",
    "<THIRD_PARTY_MODULES>",
    "^[./components]",
    "^[./]"
  ],
  "importOrderSeparation": true
}